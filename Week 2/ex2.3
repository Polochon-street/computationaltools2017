#!/bin/python

users = {
    "alice": "password1",
    "bob": "password2",
    "charlie": "password3",
    "eve": "password4"
}

# Accept login function returns True if we provide
# the right password for an existing user and False otherwise
def accept_login(users, username, password):
    if username in users and users[username] == password:
        return True
    else:
        return False

# Small tests
if accept_login(users, "alice", "password1"):
    print('Alice logged in successfully.')

if accept_login(users, "charlie", "garbage"):
    print('Access denied.')
